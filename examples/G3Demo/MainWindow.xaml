<Window x:Class="G3Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:G3Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Button Command="{Binding BrowseForGlasses}">Browse</Button>
            <Button Command="{Binding ListFrequencies}" >List Frequencies</Button>
            <Button Command="{Binding MakeCalibration}">Make calibration</Button>
            <Button Command="{Binding WebRTCSetup}" >WebRTCSetup</Button>
        </StackPanel>
        <ListBox Grid.Column="1" ItemsSource="{Binding Logs}"/>

    </Grid>
</Window>
